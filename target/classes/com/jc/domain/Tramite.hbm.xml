<?xml version="1.0" encoding="UTF-8"?>

<hibernate-mapping package="com.jc.domain">
	
	<class name="Tramite" table="Tramite">
	<!-- Mapping del Id -->
		<id name="idTramite" column="idTramite">
			<generator class="native" />
		</id>
		<property name="tipoTramite" type="string" column="tipoTramite"/>
		<property name="fechaCreacion" type="timestamp" column="fechaTramite" />
		
	</class>

</hibernate-mapping>
